<script id="tweet_template" type="text/x-jquery-tmpl">
  <li class="tweet">
    <p class="text">
      ${text}
    </p>
    <span class="user">
      <a href="http://twitter.com/${user.screen_name}" class="link">${user.name}</a>
    </span>
    <span class="date">
      ${$.timeago(created_at)}
    </span>
  </li>
</script>

<script id="video_template" type="text/x-jquery-tmpl">
