<!--
  This is the html template used to render the tweets.
  We use jQuery Template to insert data inside the html: http://api.jquery.com/jquery.tmpl/

  This is a reduced version of the tweet's JSON structure:

  created_at: "Fri Nov 04 16:32:13 +0000 2011"
  favorited: false
  id: 132495375754989570
  retweeted: false
  source: "web"
  text: "&gt;&gt; Text of the tweet"
  truncated: false
  user: Object
    name: "DrinkBODYARMOR"
    screen_name: "DrinkBODYARMOR"
-->
<script id="tweet_template" type="text/x-jquery-tmpl">
  <li class="tweet">
    <p class="text">
      ${text}
    </p>
    <span class="user">
      <a href="http://twitter.com/${user.screen_name}" class="link">${user.name}</a>
    </span>
    <span class="date">
      ${$.timeago(created_at)}
    </span>
  </li>
</script>

