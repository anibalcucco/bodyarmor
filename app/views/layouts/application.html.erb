<!DOCTYPE html>
<html>
<head>
  <title>Bodyarmor</title>
  <%= stylesheet_link_tag :all %>
  <%= stylesheet_link_tag "carousel/bodyarmor" %>
  <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?libraries=geometry&sensor=true"></script>
  <%= javascript_include_tag :defaults, "jquery.min", "jquery.tmpl.min", "jquery.jcarousel.min", "jquery.placeholder", "jquery.timeago", "jquery.maphilight.min", "bottles", "map", "videos", "twitter", "events" %>
  <%= csrf_meta_tag %>
</head>
<body>
  <%= yield %>

  <div id="fb-root"></div>
  <script type="text/javascript">
    window.fbAsyncInit = function() {
      FB.init({
        appId: '148154988610481',
        status: true,
        cookie: true,
        xfbml: true
      });
      //this resizes the the i-frame
      //on an interval of 100ms
      FB.Canvas.setAutoResize(100);
    };
    (function() {
      var e = document.createElement('script');
      e.async = true;
      e.src = document.location.protocol +
       '//connect.facebook.net/en_US/all.js';
      document.getElementById('fb-root').appendChild(e);
    }());
  </script>
</body>
</html>
